  },
  "peerDependencies": {
    "pixi.js": "^4.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "~9.0.8",
    "chai": "^2.3.0",
    "html-webpack-plugin": "^2.29.0",
    "mocha": "^2.2.4",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"

  }
