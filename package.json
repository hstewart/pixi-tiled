{
  "name": "pixi-tiled",
  "version": "1.2.0",
  "description": "A Pixi.js plugin to import maps from the Tiled map editor.",
  "keywords": [
    "pixi",
    "pixi.js",
    "tiled",
    "tilemap"
  ],
  "author": "Stuart Lee",
  "repository": "https://github.com/beeglebug/pixi-tiled",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build": "browserify index.js -o bin/pixi-tiled.js",
    "build-webpack": "node_modules/.bin/webpack --progress --colors --config webpack.config.prod.js",
    "dev": "node_modules/.bin/webpack-dev-server --progress --colors",
